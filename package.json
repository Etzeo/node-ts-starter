{
  "name": "periodic-website-screenshot",
  "version": "0.1.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint ./ --ext ts",
    "lint:fix": "eslint ./ --ext ts --fix",
